{
  "name": "cdk-template-project",
  "version": "0.1.0",
  "bin": {
    "cdk-template-project": "bin/cdk-template-project.js"
  },
  "scripts": {
    "build": "rm -rf dist && rm -fr cdk.out && yarn format && tsc && cdk synth",
    "format": "prettier --write lib/**/*.ts bin/**/*.ts tests/**/*.test.ts",
    "deploy": "yarn build && cdk deploy",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "aws-cdk": "2.84.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.84.0",
    "constructs": "^10.0.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21"
  }
}
